{
    "alias": "pickle.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": ["src"],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "atoum",
                "bin",
                "Tests",
                "tests",
                "phpunit",
                "README",
                "CHANGELOG",
                "CONTRIBUTING"
            ],
            "in": "vendor"
        }
    ],
    "git-commit": "git-commit",
    "git-version": "git-version",
    "main": "bin/pickle",
    "output": "pickle.phar",
    "stub": true
}