{
    "name": "apc",
    "type": "extension",
    "extra": {
        "configure-options": {
            "enable-apc-debug": {
                "default": "no",
                "prompt": "Enable internal debugging in APC"
            },
            "enable-apc-filehits": {
                "default": "no",
                "prompt": "Enable per request file info about files used from the APC cache"
            },
            "enable-apc-spinlocks": {
                "default": "no",
                "prompt": "Enable spin locks (EXPERIMENTAL)"
            },
            "enable-apc-memprotect": {
                "default": "no",
                "prompt": "Enable memory protection (EXPERIMENTAL)"
            },
            "enable-apc-pthreadmutex": {
                "default": "no",
                "prompt": "Enable pthread mutexes (default)"
            },
            "enable-apc-pthreadrwlocks": {
                "default": "yes",
                "prompt": "Enable pthread read\/write locks (EXPERIMENTAL)"
            }
        }
    }
}